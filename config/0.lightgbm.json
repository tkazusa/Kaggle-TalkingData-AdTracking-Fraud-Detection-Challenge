{
  "features": [
    "0.TimeInformation"
  ],
  "model": {
    "name": "lightgbm",
    "model_params": {
      "boosting_type": "gbdt",
      "objective": "binary",
      "metric": ["auc"],
      "learning_rate": 0.2,
      "num_leaves": 50,
      "max_depth": 4,
      "max_bin": 100,
      "subsample": 0.7,
      "subsample_freq": 1,
      "min_child_samples": 100,
      "min_child_weight": 0,
      "validation_ratio": 0.1,
      "verbose": 0
    },
    "train_params": {
      "num_boost_round": 250,
      "early_stopping_rounds": 100
    }
  },
  "dataset": {
    "negative_down_sampling": {
      "enabled": true,
      "bagging_size": 5,
      "random_state": 3655
    }
  }
}
